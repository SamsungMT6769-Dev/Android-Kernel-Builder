[
  {
    "kernelSource": {
      "name": "Galaxy A22 Kernel",
      "repo": "https://github.com/Samsung-MT6769-Devs/android_kernel_samsung_a22_remake",
      "branch": "fourteen_wip",
      "device": "a22",
      "defconfig": "a22_defconfig"
    },
    "withKernelSU": false,
    "toolchains": [
      {
        "repo": "https://github.com/prabhatKrMishra/android-kernel-tools/",
        "branch": "google",
        "name": "google",
        "binPath": ["./aarch64-linux-android-4.9/bin", "llvm/bin"]
